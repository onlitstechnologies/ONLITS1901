DROP FUNCTION IF EXISTS GenerateBranchIFSC;
DELIMITER //
CREATE FUNCTION GenerateBranchIFSC()
RETURNS CHAR(11)
BEGIN
DECLARE ifsc CHAR(11);
DECLARE sl INT;
SET sl = GenerateSerial();
SET sl = sl + 1;
IF(sl<10) THEN
SET ifsc = CONCAT('ABCD000000',sl);
ELSEIF(sl<100) THEN
SET ifsc = CONCAT('ABCD00000',sl);
ELSEIF(sl<1000) THEN
SET ifsc = CONCAT('ABCD0000',sl);
END IF;
RETURN ifsc;
END;
//
DELIMITER ;
